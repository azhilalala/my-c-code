#include<iostream>    
using namespace std;    
void swap(int & a, int & b)    
{    
	int t = a;    
	a = b;     
	b = t;    
}    


int main() {    
	int x, y;    
	cout << "请输入x,y的值" << endl;    
	cin >> x;    
	cin >> y;    
	swap(x,y);    
	cout << "x=" <<x<<" "<<"y="<<y<<endl;    
	return 0;    
}    
